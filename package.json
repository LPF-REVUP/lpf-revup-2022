{
  "name": "lpf-revup-2022",
  "version": "1.0.0",
  "description": "Rev Up 2022",
  "author": "jiyuujin <jiyuujinunite@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run tailwind:w\" \"svelte-kit dev\"",
    "build": "npm run tailwind:build && svelte-kit build",
    "preview": "svelte-kit preview",
    "tailwind:w": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/tailwind.css -o src/tailwind-output.css -w",
    "tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production  postcss src/tailwind.css -o src/tailwind-output.css",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "eslint . --ignore-path .gitignore --fix",
    "format": "prettier . --write --plugin-search-dir=. --ignore-path .gitignore"
  },
  "dependencies": {
    "@fontsource/fira-mono": "4.5.8",
    "@line/liff": "2.20.2",
    "@lukeed/uuid": "2.0.0",
    "cookie": "0.5.0",
    "dayjs": "1.11.3"
  },
  "devDependencies": {
    "@poppanator/sveltekit-svg": "0.3.4",
    "@sveltejs/adapter-netlify": "1.0.0-next.64",
    "@sveltejs/kit": "1.0.0-next.350",
    "@sveltejs/svelte-virtual-list": "3.0.1",
    "@types/cookie": "0.5.1",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "autoprefixer": "10.4.7",
    "concurrently": "6.5.1",
    "cross-env": "7.0.3",
    "csstype": "3.1.0",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "3.4.1",
    "eslint-plugin-tailwindcss": "3.5.2",
    "postcss": "8.4.14",
    "postcss-cli": "9.1.0",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.0",
    "svelte": "3.48.0",
    "svelte-check": "2.7.2",
    "svelte-preprocess": "4.10.7",
    "tailwindcss": "3.1.4",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "type": "module"
}
